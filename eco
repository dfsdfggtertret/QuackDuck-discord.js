
const Discord = require('discord.js')
const db = require('quick.db')

module.exports.run = async (bot, message, args) => {



    if (!message.member.hasPermission('ADMINISTRATOR')) {
        return message.reply('У вас недостаточно прав')
    }

    if (!args[0]) return message.reply('Укажите число')
    if (isNaN(args[0])) return message.reply('Укажите число, больше 0')

    let user = message.mentions.users.first() || message.author
    message.channel.send('Добавлено успешно' + args[0] + ' to ' + user)
    db.add(`money_${message.guild.id}_${message.author.id}`, args[0])

};
module.exports.help = {
  name: 'eco'
};
